{
  "name": "startpage-addons",
  "version": "1.0.0",
  "description": "Startpage Browser Addon",
  "main": "index.js",
  "dependencies": {
    "web-ext": "^7.11.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:firefox": "web-ext build -s firefox --overwrite-dest -i \"*.zip\" -a firefox/web-ext-artifacts",
    "run:firefox": "web-ext run -s firefox -i \"*.zip\" -a firefox/web-ext-artifacts",
    "sign:firefox": "web-ext sign -s firefox --overwrite-dest -i \"*.zip\" -a firefox/web-ext-artifacts",
    "lint:firefox": "web-ext lint -s firefox -i \"*.zip\"",
    "build:chrome": "web-ext build -schrome --overwrite-dest -i \"*.zip\" -a chrome/web-ext-artifacts",
    "lint:chrome": "web-ext lint -s chrome -i \"*.zip\""
  },
